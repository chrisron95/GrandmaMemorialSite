<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @post.name %>
</p>

<p>
  <strong>Message:</strong>
  <%= @post.message %>
</p>

<% @post.images.each do |image| %>
  <%= image_tag image %> <br />
<% end %>

<%= link_to 'Edit', edit_post_path(@post) %> |
<%= link_to 'Back', posts_path %>
